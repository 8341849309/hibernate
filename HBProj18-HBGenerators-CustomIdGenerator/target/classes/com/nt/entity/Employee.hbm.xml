<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.nt.entity.Employee" table="EMPLOYEE_GENERATORS_CUSTOM_GENERATOR"
		dynamic-update="true" dynamic-insert="true">
 
 		<!-- Generated by DB s/w suitabl ein cluster -->
		<id name="empid" column="EMPLOYEE_ID" type="long">
			<generator class="com.nt.generator.RandomIdGenerator"/>
		</id>
 		
 	
		<property name="name" column="EMPLOYEE_NAME" type="string"
			length="25" />
		<property name="salary" column="SALARY" type="float" />

	</class>

</hibernate-mapping>