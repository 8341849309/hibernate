<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>

	<class name="com.nt.entity.Hql_Insert_Test" table="HQL_INSERT_TEST" dynamic-insert="true" lazy="false">
		
		<id name="id" type="int">
			<generator class="increment"/>
		</id>
		
		<property name="name" type="string" length="20"/>
		<property name="addrs" type="string" length="20"/>
		<property name="phone" type="long"/>
		<property name="salary" type="float"/>
		
	</class>

</hibernate-mapping>